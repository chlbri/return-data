  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "core": "github:chlbri/core#v0.0.22",
    "dayjs": "^1.10.7",
    "dequal": "^2.0.2",
    "edit-json-file": "^1.6.0",
    "eslint": "^8.0.1",
    "faker": "^5.5.3",
    "immer": "^9.0.6",
    "jest": "^27.3.1",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^1.1.0",
    "nanoid": "^3.1.30",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "zod": "3.10.1"
  },
  "peerDependencies": {
    "dequal": "^2.0.2",
    "immer": "^9.0.6",
    "nanoid": "^3.1.30",
    "zod": "3.10.1"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm run upgrade-internal && pnpm run test && pnpm run tsc && pnpm run lint",
    "build:-test": "pnpm run tsc && pnpm run lint",
    "test": "jest --passWithNoTests",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx,yml,json}\"",
    "lint": "pnpm run prettier && pnpm run eslint -- --fix",
    "rm": "./node_modules/.bin/rimraf -rf node_modules pnpm.lock",
    "rinit": "pnpm run rm && pnpm cache clean && pnpm run pnpm",
    "publish": "pnpm run rm && pnpm run clean && pnpm run pnpm",
    "clean": "pnpm cache clean",
    "pnpm": "pnpm install",
    "upgrade": "pnpm upgrade",
    "upgrade-internal": "pnpm upgrade core",
    "git-push": "git push --follow-tags",
    "git-build-push": "pnpm install && pnpm build && pnpm run git-push",
    "git-publish": "pnpm run git-build-push && git checkout main && git merge dev && pnpm run git-push && git checkout dev"
  },
  "dependencies": {
    "vitest": "^1.5.3"
  }
}
