{
  "include": ["src/**/*"],
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "importHelpers": true,
    "target": "ESNext",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./lib",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "Bundler",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "declarationMap": true,
    "paths": {
      "#schemas/*": ["./src/schemas/*"],
      "#status": ["./src/constants/status/index.ts"],
      "#status/*": ["./src/constants/status/*"],
      "#data/*": ["./src/constants/rd/*"],
      "#data": ["./src/constants/rd/index.ts"],
      "#rd": ["./src/ReturnData.ts"],
      "#typings": ["./src/types.ts"],
      "#functions/*": ["./src/functions/*"],
      "#lib": ["./lib"],
      "#lib/*": ["./lib/*"],
      "#fixtures": ["./src/fixtures/index.ts"],
      "#fixtures/*": ["./src/fixtures/*"]
    }
  }
}
